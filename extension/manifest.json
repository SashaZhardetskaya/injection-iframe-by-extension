{
  "key": "",
  "manifest_version": 2,
  "name": "IframeInjection",
  "version": "1.0.0",
  "description": "Protect your identity with the smartest privacy solution ever",
  "icons": {
    "16": "images/icon-16.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "",
    "default_icon": {
      "16": "images/icon-16.png"
    },
    "default_title": "FigLeaf"
  },
  "permissions": [
    "privacy",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "activeTab",
    "file:///*",
    "nativeMessaging",
    "<all_urls>",
    "storage",
    "proxy"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg",
    "fonts/*.woff"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; font-src * "
}
